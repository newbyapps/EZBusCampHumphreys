<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jtnew.ezBusCampHumphreys">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bus_new"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.jtnew.ezBusCampHumphreys.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jtnew.ezBusCampHumphreys.BlueWeekday"
            android:label="@string/title_activity_blue"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name="com.jtnew.ezBusCampHumphreys.BlueWeekend"
            android:label="@string/title_activity_blue"
            android:screenOrientation="portrait"></activity>
    <activity
        android:name="com.jtnew.ezBusCampHumphreys.RedWeekday"
        android:label="@string/title_activity_red"
        android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.jtnew.ezBusCampHumphreys.RedWeekend"
            android:label="@string/title_activity_red"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.jtnew.ezBusCampHumphreys.GreenWeekend"
            android:label="@string/title_activity_green"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.jtnew.ezBusCampHumphreys.GreenWeekday"
            android:label="@string/title_activity_green"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>